p Renders sub groups as tabs
+example#example-tabs-1
  script(type="text/javascript").language-javascript.
    TweakUi.mount("#example-tabs-1", (ui) => {
      ui.tabs((tabs) => {
        ui.group("Tab 1", () => {
          ui.button("Button")
        })
        ui.group("Tab 2", () => {
          ui.number({ label: "Number" })
        })
        ui.group("Tab 3", () => {
          ui.text({ label: "Text" })
        })
      })
    })
