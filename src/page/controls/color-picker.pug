+example#example-color-picker
  script(type="text/javascript").language-javascript.
    var context = { value: '#fff' }
    TweakUi.mount("#example-color-picker", (ui) => {
      ui.colorPicker(context, 'value', {
        label: 'Color',
        onInput: console.log,
        onChange: console.log,
      })
    })
